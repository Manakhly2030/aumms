 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <style>\n        table {\n            border-collapse: collapse;\n        }\n        th, td {\n            border: 1px solid black;\n            padding: 8px;\n        }\n        .tall {\n            height: 100px;\n        }\n        .wide {\n            width: 150px;\n        }\n    </style>\n</head>\n<body>\n    <table style=\"width:100%;    font-size: 12px;\">\n        <thead>\n            <tr>\n                <td colspan=\"8\"><div style=\"text-align: center;\"><b><u>Sales Estimate</u></b></div></td>\n            </tr>\n        </thead>\n        <tbody>\n            <tr>\n                <td colspan=\"3\"><b>{{ _(\"Customer Name\") }}:</b> {{ doc.customer_name }} </td>\n                <!--<td><b>Board Rate @916</b></td>-->\n                <!--<td><b>{{ doc.items[0].board_rate }}</b></td>-->\n                {% set gst = frappe.get_doc(\"Company\",company) %}\n                <td rowspan=\"2\" colspan=\"6\"><div style=\"text-align: right;\"><b>{{ _(\"GSTIN\") }}:</b>{{ gst.gstin }}<br><b>{{ _(\"Date\") }}:</b>{{ doc.get_formatted(\"transaction_date\") }}</div></td>\n            </tr>\n            <!--<tr>-->\n            <!--    <td colspan=\"3\"><b>{{ _(\"Customer Name\") }}:</b> {{ doc.customer_name }}<br><b>NAME</b><br><b>C/O RFQ</b><br>98</td>-->\n            <!--</tr>-->\n        </tbody>\n    </table>\n    <table style=\"font-size: 12px;width:100%;\">\n        <tr>\n            <td><b>Sl. No.</b></td>\n            <td colspan=\"2\"><b>Item</b></td>\n            <!--<td><b>Qty</b></td>-->\n            <td><b>Gr. Wt</b></td>\n            <td><b>St. Wt</b></td>\n            <td><b>Nt Wt</b></td>\n            <td><b>MC %</b></td>\n            <td><b>Total</b></td>\n        </tr>\n        {%- for item in doc.items -%}\n         <tr>\n            <td>{{ item.idx }}</td> \n            <td colspan=\"2\">{{ item.item_code }}</td>\n            <!--<td></td>-->\n            <td>{{ item.gold_weight }}</td>\n            <td>{{ item.stone_weight }}</td>\n            <td>{{ item.net_weight }}</td>\n            <td>{{ item.making_charge_percentage }}%</td>\n            <td>{{ item.amount }}</td>\n        </tr>\n        {%- endfor -%}\n        <tr>\n            <td colspan=\"8\">Pearl Charge</td>\n        </tr>\n        <tr>\n            <td colspan=\"8\">Kunthan Charge</td>\n        </tr>\n        <tr>\n            <td colspan=\"8\">Stone charge</td>\n        </tr>\n        <tr>\n            <td></td>\n            <td colspan=\"2\"><b>Total</b></td>\n            <td>{{ doc.total_gold_weight }}</td>\n            <td>--</td>\n            <td>--</td>\n            <td>--</td>\n            <td><b>{{ doc.total_gold_amount}}</b></td>\n        </tr>\n        <tr>\n            <td colspan=\"8\" style=\"border-bottom: none;\"></td>\n        </tr>\n        </table>\n        <table style=\"font-size: 12px;width:100%;\">\n             <tr>\n                <td colspan=\"6\">Old Gold Received</td>\n                <td></td>\n                <td rowspan=\"6\" style=\"border-bottom: none; border-top: none; border-left: none;\"></td>\n            </tr>\n            <tr>\n                <td class=\"wide\"><b>ITEM</b></td>\n                <td><b>Purity</b></td>\n                <td><b>Gr. Wt</b></td>\n                <td><b>Stone Wt.</b></td>\n                <td><b>Net Weight</b></td>\n                <td><b>Rate</b></td>\n                <td><b>Amount</b></td>\n            </tr>\n            {%- for item in doc.old_jewellery_items -%}\n            <tr>\n                <td class=\"wide\">Old Gold Received</td>\n                <td>{{ item.purity }}</td>\n                <td>0.000</td>\n                <td>0.000</td>\n                <td>0.000</td>\n                <td>{{ item.rate }}</td>\n                <td>{{ item.amount }}</td>\n            </tr>\n            {%- endfor -%}\n            <tr>\n                <td class=\"wide\"></td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td></td>\n            </tr>\n             <tr>\n                <td class=\"wide\"></td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td>{{ doc.total_old_gold_amount }}</td>\n            </tr>\n            <tr>\n                <td colspan=\"8\" style=\"border-top:1px solid #fff; border-bottom:1px solid #fff;\"></td>\n            </tr>\n        </table>\n        <table style=\"width:100%;\">\n            <tr>\n            <td style=\"border-top: none; border-bottom: none;\">\n            <table style=\"float:left; width:30%; margin-left:-11px;font-size: 12px;\">\n                <tr>\n                    <td colspan=\"2\"><b>DETAILS OF ADVANCE</b></td>\n                </tr>\n                <tr>\n                    <td>AMOUNT</td>\n                    <td>RATE</td>\n                </tr>\n                <tr>\n                    <td></td>\n                    <td></td>\n                </tr>\n            </table>\n            <table style=\"float:right; width:50%; margin-right:-11px; font-size: 12px;\">\n                <tr>\n                    <td colspan=\"2\">Advance received</td>\n                    <td>{{ doc.total_advance_received }}</td>\n                </tr>\n                <tr>\n                    <td colspan=\"2\">Old received</td>\n                    <td>{{ doc.total_old_gold_amount}}</td>\n                </tr>\n                <tr>\n                    <td colspan=\"2\">Discount on advance payment</td>\n                    <td>0</td>\n                </tr>\n                <tr>\n                    <td colspan=\"2\"><b>Balance Receivable/ Payable</b></td>\n                    <td><b>{{ doc.outstanding_amount }}</b></td>\n                </tr>\n            </table>\n            </td>\n            </tr>\n        </table>\n        <table style=\"width:100%;\">\n             <tr>\n                <td colspan=\"8\" class=\"tall\" style=\"border-top: none;\"><div style=\"text-align: right;\">Authorised Signatory</div></td>\n            </tr>\n        </table>\n</body>\n</html>\n",
